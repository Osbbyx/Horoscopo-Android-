            android:name="lol.lol.horoscopo.Acuario"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.capricornio"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="lol.lol.horoscopo.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
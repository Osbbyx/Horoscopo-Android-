        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        
        <activity android:name="lol.lol.horoscopo.about" />
        <activity
            android:name="lol.lol.horoscopo.Sagitario"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.Escorpio"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.Libra"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.Virgo"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.Leo"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.Cancer"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.Geminis"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.Tauro"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.Aries"
            android:screenOrientation="portrait" />
        <activity
            android:name="lol.lol.horoscopo.Piscis"
            android:screenOrientation="portrait" />
